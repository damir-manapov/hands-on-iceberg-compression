{
  "name": "hands-on-iceberg-compression",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "rmcheckpoints": "rm -rf .checkpoints",
    "rebench": "yarn rmcheckpoints & yarn bench",
    "bench": "tsx src/bench.ts",
    "plot": "tsx src/plot.ts",
    "bench:all": "yarn bench && yarn plot",
    "build": "tsc -p tsconfig.json",
    "lint": "tsc --noEmit",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "compose:up": "docker compose up -d",
    "compose:down": "docker compose down",
    "compose:restart": "yarn compose:down && yarn compose:up",
    "compose:logs": "docker compose logs -f --tail=300",
    "compose:ps": "docker compose ps",
    "compose:clean": "docker compose down -v --remove-orphans"
  },
  "author": "Damir Manapov",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "chart.js": "^4.5.0",
    "chartjs-node-canvas": "^5.0.0",
    "csv-parse": "^6.1.0",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
